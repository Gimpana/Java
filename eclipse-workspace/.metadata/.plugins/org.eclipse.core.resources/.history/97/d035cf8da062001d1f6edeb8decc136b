package PAC_DESARROLLO_UF5;

public class Gasto extends Dinero {
	
	public Gastos(double dinero, String description) {
		
		super(dinero, description);
	}
	
	public void Gasto(double gasto, String description) {
		
		this.dinero -= gasto;
		this.description = description;
	}
	
	@Override
	public String toString() {
		
		return "Dinero = " + dinero +
				"Description = " + description; 
	}

}
